{
  "extends": "next/core-web-vitals",
  "rules": {
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          ["parent", "sibling", "index"]
        ],
        "pathGroups": [
          {
            "pattern": "next/*",
            "group": "external"
          }
        ],
        "pathGroupsExcludedImportTypes": ["next/*"]
      }
    ],
    "no-unused-vars": "error"
  }
}
